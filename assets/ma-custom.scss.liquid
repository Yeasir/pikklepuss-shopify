/* 1. Ensure this sits above everything when visible */
.custom-modal {
  position: absolute;
  z-index: 10000; /* 1 */
  top: 0;
  left: 0;
  visibility: hidden;
  width: 100%;
  height: 100%;
  display: none
}

.custom-modal.is-visible {
  visibility: visible;
  display: block
}

.custom-modal-overlay {
position: fixed;
top: 0;
left: 0;
right: 0;
bottom: 0;
z-index: 10;
background:#ebebeb;
visibility: hidden;
opacity: 0;
transition: visibility 0s linear 0.3s, opacity 0.3s;
}

.custom-modal.is-visible .custom-modal-overlay {
opacity: 1;
visibility: visible;
transition-delay: 0s;
}

.custom-modal-wrapper {
position: fixed;
z-index: 9999;
top: 30%;
left: 50%;
