  }
  
   .slick-next::before, .slick-prev::before {
    font-family: "slick-icons, sans-serif";
    font-size: 24px;
    line-height: 1;
    color: #ffd166;
    opacity: 1;
   }
   .slick-prev{
    text-align:right;
  }
  .slick-next{
    text-align:left;
  }
  .slick-disabled{
    opacity: 0.4;
  }

  .slick-dots{
    display: flex !important;
    justify-content: center;
    list-style: none;
    bottom: -8%;
    margin: 20px 0;
  }
  .slick-dots li>button{
        border-radius: 40px;
        border: 1px solid #34b233;
        width: 10px;
        height: 10px;
        text-align: center;
        cursor: pointer;
        padding: 0;
        color: #000;
        font-size: 16px;
        display: block;
        line-height: 28px;
        margin: 0 5px;
        text-indent: -9999999px;
        opacity: 0.5;
        &:hover{
            border: 1px solid #34b233;
            background-color: #34b233;
            color: #fff;
            opacity:1;
        }

    }


    .slick-dots li.slick-active button{
                border: 1px solid #34b233;
                background-color: #34b233;
                color: #fff;
                opacity:1;
        }
  
}

